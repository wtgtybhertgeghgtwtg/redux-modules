{
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^21.2.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-use-lodash-es": "^0.1.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "cp-file": "^5.0.0",
    "cross-env": "^5.0.5",
    "eslint": "^4.7.2",
    "eslint-config-fb-strict": "^21.2.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.36.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jasmine": "^2.8.4",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prefer-object-spread": "^1.2.1",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-relay": "^0.0.8",
    "flow-bin": "^0.55.0",
    "flow-copy-source": "^1.2.1",
    "flux-standard-action": "^1.1.0",
    "glob": "^7.1.2",
    "jest": "^21.2.1",
    "lerna": "^2.2.0",
    "lodash": "^4.2.1",
    "make-dir": "^1.0.0",
    "prettier": "^1.7.2",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "rimraf": "^2.6.2"
  },
  "engines": {
    "node": ">=4",
    "yarn": ">=1"
  },
  "jest": {
    "testMatch": ["**/*.test.js"]
  },
  "private": "true",
  "scripts": {
    "build": "node ./scripts/build",
    "clean": "rimraf packages/**/es packages/**/lib",
    "flow": "flow",
    "jest": "jest --coverage",
    "lint": "eslint packages/**/src/*.js packages/**/__tests__/*.js",
    "lint:fix": "eslint packages/**/*.js --fix",
    "prettier:lint": "prettier --l \"packages/**/*.js\"",
    "prettier:write": "prettier --write \"packages/**/*.js\"",
    "publish": "yarn run clean && yarn run build && lerna publish",
    "test": "yarn run lint && yarn run flow && yarn run jest"
  },
  "workspaces": ["packages/*"]
}
